{
    "name": "mekras/obereg",
    "description": "PHP fault tolerance library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Михаил Красильников",
            "email": "m.krasilnikov@yandex.ru"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.1",
        "php-http/httplug": "^1.0",
        "php-http/message": "^1.0",
        "php-http/message-factory": "^1.0",
        "psr/cache": "^1.0.1"
    },
    "require-dev": {
        "cache/array-adapter": "^0.4",
        "doctrine/cache": "^1.6",
        "guzzlehttp/psr7": "^1.2",
        "phpunit/phpunit": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "Mekras\\Obereg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mekras\\Obereg\\Tests\\": "tests"
        }
    }
}